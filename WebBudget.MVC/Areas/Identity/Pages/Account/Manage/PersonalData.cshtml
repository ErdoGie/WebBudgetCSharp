@page
@model PersonalDataModel
@{
    ViewData["Title"] = "Personal Data";
    ViewData["ActivePage"] = ManageNavPages.PersonalData;
}

<h3>@ViewData["Title"]</h3>

<div class="row" style="justify-content:center; text-align:center">
    <div class="col-md-6" >
        <p>Your account contains personal data that you have inserted earlier.</p>
        <p>
            <strong>By deleting your account, your data, incomes and expenses will be removed!</strong>
        </p>
    
        <p>
            <a id="delete" asp-page="DeletePersonalData" class="btn btn-danger">Go</a>
        </p>
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
